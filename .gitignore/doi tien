
#include <iostream>
#include <cmath>

using namespace std;

int main(int argc, char** argv) {
	int n;
	int dem10 = 0;
	int dem5 = 0;
	int dem2 = 0;
	int dem1 = 0;
	
	cout<<"nhap so tien can qui doi : ";
	cin>>n;
	
	dem10 = n/10;
	dem5 = (n - dem10 * 10)/5;
	dem2 = (n- dem10 * 10 - dem5 * 5)/2;
	dem1 = (n - dem10 * 10 - dem5 * 5 - dem2 * 2);
	
	cout<<"so to 10d: "<<dem10<<endl;
	cout<<"so to 5d: "<<dem5 <<endl;
	cout<<"so to 2d: "<<dem2<<endl;
	cout<<"so to 1d: "<<dem1;
	return 0;
}
